    "apollo-boost": "^0.1.12",
    "apollo-cache-persist": "^0.1.1",
    "apollo-link-state": "^0.4.1",
    "autosuggest-highlight": "^3.1.1",
    "classnames": "^2.2.6",
    "disqus-react": "^1.0.5",
    "formik": "^1.0.2",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "jquery": "^3.3.1",
    "localforage": "^1.7.2",
    "node-sass-chokidar": "^1.3.3",
    "pure-react-carousel": "^1.13.2",
    "rc-slider": "^8.6.1",
    "react": "^16.4.2",
    "react-apollo": "^2.1.9",
    "react-async-script-loader": "^0.3.0",
    "react-autosuggest": "^9.4.0",
    "react-dom": "^16.4.2",
    "react-froala-wysiwyg": "^2.8.4",
    "react-intl": "^2.4.0",
    "react-player": "^1.6.4",
    "react-popper": "^0.10.2",
    "react-router-dom": "^4.3.1",
    "react-s3-uploader": "^4.8.0",
    "react-scripts": "1.1.4",
    "react-share": "^2.2.0",
    "uuidv4": "^1.0.1",
    "yup": "^0.26.0"
  },
  "scripts": {
    "deploy": "npm run build && set AWS_SHARED_CREDENTIALS_FILE=./credentials && aws s3 sync --acl public-read --delete build/ s3://cv30.co && npm run invalidate",
    "invalidate": "set AWS_SHARED_CREDENTIALS_FILE=./credentials && aws cloudfront create-invalidation --distribution-id E37006VW3U4LCL --paths /*",
    "build-css": "node-sass-chokidar src/style -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/style -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3"
  }
}
